package ex01;

public class MyExcept {
	
	public void makeException(int num) {
		
		if(num == 1) {
			throw new ClassCastException();
		}
		else if(num == 2) {
			throw new NullPointerException();
		}
		else if(num == 3) {
			throw new ArrayIndexOutOfBoundsException();
		}
		else if(num == 4) {
			throw new ArithmeticException();
		}
	}

}
