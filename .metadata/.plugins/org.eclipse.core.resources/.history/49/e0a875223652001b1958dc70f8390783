package test;

import java.util.Scanner;



public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int N = input.nextInt();
		
		for(int i =0; i<= N; i++) {
			star();
		}
		
		}
	

public static String star() {
	System.out.println("*");
	star();
	return "*";
	}


}


