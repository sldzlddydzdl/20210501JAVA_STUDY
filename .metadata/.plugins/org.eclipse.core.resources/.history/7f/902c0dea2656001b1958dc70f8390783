		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		
		int result = A*B*C;
		
		int ar[] = new int[10];
		
		for(int i =0; i < 10; i++) {
			for(int k = 100000000; k >= 1; k = k/10){
			if((result / k) == i) {
				result = result - (result/k)*k;
				System.out.println(result);
				ar[i] += 1;
				}
			}		
		}
		
		for(int j =0; j<10; j++) {
			System.out.println(ar[j]);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
