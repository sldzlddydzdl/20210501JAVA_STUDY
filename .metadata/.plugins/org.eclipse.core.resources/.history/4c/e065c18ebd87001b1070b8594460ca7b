package main;

import java.util.*;

public class Main {

		public static void main(String[] args) {
		
			Scanner sc = new Scanner(System.in);
			
			int N = sc.nextInt();
			int N3 = N;
			int N5 = N;
			boolean posi = true;
			
			while(N3 > 0 && N5 >0) {
				
				N3 = N3 -3;
				N5 = N5 -5;
			}
			
			if(N3 == 0 ) posi = true;
			if(N3 < 0) {
				posi = false;
				System.out.println(-1);
			}
			if(N5 == 0 ) posi = true;
			if(N5 < 0) {
				posi = false;
				System.out.println(-1);
			}
			
			
			
			
			
		}
}
