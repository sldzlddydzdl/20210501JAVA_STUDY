package test;

import java.util.Scanner;



public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	      Scanner sc = new Scanner(System.in);
	      
	      int N = sc.nextInt();
	      
	      star(N, N);
	      

	   }
	   
	   public static void star(int N, int count) {
	      
	      for(int i=--count; i<N; i++)
	         System.out.print("*");
	      System.out.println();
	      
	      if(count>0)
	         star(N, count);
	      
	   }

}


