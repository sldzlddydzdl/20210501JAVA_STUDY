package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);

		int n = input.nextInt();
		
		for(int i = 1; i <=  n; i++) {
			int y;
			y =+ i;
			if(i == n) {
				System.out.println(y);
			}
		}
	
		
		
	}

}
