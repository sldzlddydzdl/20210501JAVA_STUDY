package ex01;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int ob = sc.nextInt();
		String str = "김동현";
		if(str.equals(ob)) {
			System.out.println("이름이 같습니다.");
		}
	}

}
