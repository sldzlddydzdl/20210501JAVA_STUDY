package ex01;

public class Ex01 {

	public static void main(String[] args) {
		// 예외(exception) : 잘못된 코드, 부정확한 데이터, 예외적인 상황에 의하여 발생
		
		try {
			System.out.println(10/0);
		}
		catch (Exception e) {
			// TODO: handle exception
			System.out.println("숫자를 0으로나눌수없습니당!");
		}
		System.out.println("끝");
	}
}
