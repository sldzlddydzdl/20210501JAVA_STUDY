package test;

import java.util.Scanner;



public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		
		int N = input.nextInt();
		String output = "";
		for(int i = 1; i <= N; i++) {
			
			output += "*";
			System.out.println(output);
		}
		
	}

}

